{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "jsx": "react-jsx",
      "types": ["jest", "@testing-library/jest-dom"],
      "esModuleInterop": true,
      "module": "ESNext",
      "target": "ESNext"
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "**/*.test.ts",
      "**/*.test.tsx",
      "jest.config.cjs",
      "src/setupTests.ts"
    ]
  }